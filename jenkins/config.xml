<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.TextParameterDefinition>
          <name>VERSION</name>
          <description></description>
          <defaultValue></defaultValue>
        </hudson.model.TextParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.plugins.git.GitSCM" plugin="git@2.0">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <url>/Users/droberts/github/droberts2013/XebiaLabsDemoApp</url>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>*/master</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <submoduleCfg class="list"/>
    <extensions/>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>cd tomcat
mvn package
</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <com.xebialabs.deployit.ci.DeployitNotifier plugin="deployit-plugin@4.5.2">
      <credential>admin</credential>
      <application>Applications/CoolApps/xlda</application>
      <version>1.${VERSION}.$BUILD_NUMBER</version>
      <packageOptions>
        <deployables>
          <com.xebialabs.deployit.ci.ArtifactView>
            <type>tomcat.War</type>
            <name>xlda-tomcat</name>
            <tags></tags>
            <properties>
              <com.xebialabs.deployit.ci.NameValuePair>
                <propertyName>contextRoot</propertyName>
                <propertyValue>xlda</propertyValue>
              </com.xebialabs.deployit.ci.NameValuePair>
            </properties>
            <location>tomcat/target/xlda-0.0.1-SNAPSHOT.war</location>
          </com.xebialabs.deployit.ci.ArtifactView>
          <com.xebialabs.deployit.ci.ArtifactView>
            <type>sql.SqlScripts</type>
            <name>xlda-sql</name>
            <tags></tags>
            <location>sql</location>
          </com.xebialabs.deployit.ci.ArtifactView>
        </deployables>
      </packageOptions>
      <importOptions>
        <mode class="com.xebialabs.deployit.ci.GeneratedLocation"/>
      </importOptions>
      <deploymentOptions>
        <environment>Environments/Cloud/xlda-test-jenkins</environment>
        <generateDeployedOnUpgrade>true</generateDeployedOnUpgrade>
        <skipMode>false</skipMode>
        <testMode>false</testMode>
        <rollbackOnError>false</rollbackOnError>
        <versionKind>Packaged</versionKind>
      </deploymentOptions>
      <verbose>false</verbose>
    </com.xebialabs.deployit.ci.DeployitNotifier>
  </publishers>
  <buildWrappers/>
</project>